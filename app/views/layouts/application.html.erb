<!DOCTYPE html>
<html>
  <head>
    <title>PunchStopper</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <h1  align="center">
    <%= image_tag("logos/logo.png", size: "200x35", alt: "Edit Entry")%>
    </h1>
    <p align="center">Where the punches stop, and the kicks start</p>
		<%= link_to "Explore", projects_path %>
		<% if current_user %>
			<%= link_to "My Projects", projects_path(my_projects: true) %>
			<%= link_to "Wishlist", user_wishlists_path %>
			<%= link_to "Profile", current_user %>
            <% if current_user.role %>
                <%= link_to "Management", admin_path %>
            <% end %>
            <%= link_to "Sign Out", destroy_user_session_path, method: :delete %>
		<% else %>
			<%= link_to "Sign In", new_user_session_path %>
		<% end %>
  </head>

  <body>
	<p class="notice"><%= notice %></p>
	<p class="alert"><%= alert %></p>
    <%= yield %>
  </body>
</html>
