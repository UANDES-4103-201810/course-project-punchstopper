<p id="notice"><%= notice %></p>

<h1>User Wishlists</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_wishlists.each do |user_wishlist| %>
      <tr>
        <td><%= user_wishlist.project.title %></td>
        <td><%= link_to 'Go to Project', user_wishlist.project %></td>
        <td><%= link_to 'Delete', user_wishlist, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User Wishlist', new_user_wishlist_path %>
